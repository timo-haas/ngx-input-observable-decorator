{
  "name": "ngx-input-observable-decorator",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "npm run build-lib",
    "test": "ng test --watch",
    "travis": "ng build ngx-input-observable-decorator && ng test ngxInputObservableDecorator --browsers Firefox",
    "watch-lib": "ng build ngx-input-observable-decorator --watch",
    "build-lib": "npm run copy-readme-to-lib-folder && ng build ngx-input-observable-decorator",
    "build-lib-prod": "npm run copy-readme-to-lib-folder && ng build ngx-input-observable-decorator --configuration production",
    "test-lib": "ng test ngx-input-observable-decorator",
    "test-lib-ci": "ng test ngx-input-observable-decorator --watch=false --browsers=ChromeHeadless",
    "lint-lib": "ng lint ngx-input-observable-decorator",
    "pack-lib": "npm run build-lib-prod && npm pack ./dist/ngx-input-observable-decorator",
    "publish-lib": "npm run build-lib-prod && npm publish ./dist/ngx-input-observable-decorator",
    "publish-lib-next": "npm run build-lib && npm publish --tag next ./dist/ngx-input-observable-decorator",
    "copy-readme-to-lib-folder": "copyfiles README.md ./projects/ngx-input-observable-decorator"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "13.1.1",
    "@angular/common": "13.1.1",
    "@angular/compiler": "13.1.1",
    "@angular/core": "13.1.1",
    "@angular/forms": "13.1.1",
    "@angular/platform-browser": "13.1.1",
    "@angular/platform-browser-dynamic": "13.1.1",
    "@angular/router": "13.1.1",
    "core-js": "3.6.5",
    "rxjs": "6.6.3",
    "tslib": "^2.3.1",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "13.1.2",
    "@angular/cli": "13.1.2",
    "@angular/compiler-cli": "13.1.1",
    "@angular/language-service": "13.1.1",
    "@types/jasmine": "~3.6.0",
    "@types/jasminewd2": "2.0.8",
    "@types/node": "14.11.2",
    "copyfiles": "2.3.0",
    "codelyzer": "^6.0.0",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "6.3.9",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "3.0.3",
    "karma-firefox-launcher": "1.3.0",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "ng-packagr": "13.1.2",
    "protractor": "7.0.0",
    "ts-node": "9.0.0",
    "tslint": "6.1.3",
    "typescript": "4.5.4"
  }
}
